program Configuracoes;

uses
  Vcl.Forms,
  TelaConfiguracoes in '..\TelaConfiguracoes.pas' {Configuracoes},
  Vcl.Themes,
  Vcl.Styles;

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  TStyleManager.TrySetStyle('Glow');
  Application.CreateForm(TConfiguracoes, Configuracoes);
  Application.Run;
end.
